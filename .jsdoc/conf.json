{
	"plugins": [],
	"recurseDepth": 10,
	"source": {
		"exclude": [
			"node_modules",
			"src/base.js"
		],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": false,
		"dictionaries": [
			"jsdoc",
			"closure"
		]
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": false
	},
	"opts": {
		"encoding": "utf8",
		"package": "./package.json",
		"readme": "./README.md",
		"destination": "./docs/",
		"recurse": true
	}
}